---
layout: null
---

<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd">
  {% for page in site.pages %}
    {% if page.url != '/404.html' %}
      <url>
        <loc>{{ site.url }}{{ page.url | replace:'index.html','' | replace:'.html','' }}</loc>
        {% if page.priority %}
        <priority>{{ page.priority }}</priority>
        {% endif %}
      </url>
    {% endif %}
  {% endfor %}
  {% for post in site.posts %}
    <url>
      <loc>{{ site.url }}{{ post.url | replace:'.html','' }}</loc>
      {% if post.priority %}
      <priority>{{ post.priority }}</priority>
      {% endif %}
    </url>
  {% endfor %}
  <url>
    <loc>https://copperhead.co/android/docs/technical_overview</loc>
    <priority>1.0</priority>
  </url>
  <url>
    <loc>https://copperhead.co/android/docs/building</loc>
    <priority>1.0</priority>
  </url>
  <url>
    <loc>https://copperhead.co/android/docs/install</loc>
    <priority>1.0</priority>
  </url>
</urlset>
